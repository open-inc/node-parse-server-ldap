{
  "compilerOptions": {
    "lib": ["ES2019"],
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
